{
  "name": "ngrx-typed-actions",
  "version": "7.0.1",
  "description": "Actions and Reducer Utility Library for NGRX",
  "main": "dist/index.js",
  "scripts": {
    "build": "ng-packagr -p package.json",
    "test": "ngc -p tsconfig.spec.json && jasmine dist/spec/*.spec.js",
    "publish": "npm publish dist",
    "precommit": "lint-staged",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --parser typescript --single-quote es5 --write \"**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmiskiewicz/ngrx-typed-actions.git"
  },
  "keywords": [
    "ngrx",
    "redux",
    "state",
    "store",
    "state-mangement"
  ],
  "author": "Austin McDaniel, Krzysztof Mi≈õkiewicz, Arturn Maciejaszek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kmiskiewicz/ngrx-typed-actions/issues"
  },
  "homepage": "https://github.com/kmiskiewicz/ngrx-typed-actions#readme",
  "peerDependencies": {
    "@angular/core": "^7.2.6",
    "@ngrx/effects": "^7.2.0",
    "@ngrx/store": "^7.2.0",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@angular/compiler": "^7.2.6",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/core": "^7.2.6",
    "@ngrx/effects": "^7.2.0",
    "@ngrx/store": "^7.2.0",
    "@types/jasmine": "^3.3.9",
    "husky": "^1.3.1",
    "jasmine": "^3.3.1",
    "lint-staged": "^8.1.4",
    "ng-packagr": "^4.7.1",
    "prettier": "^1.16.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "tsickle": "^0.34.3",
    "tslint": "^5.12.1",
    "typescript": "~3.2.4",
    "zone.js": "^0.8.29"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run prettier",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2
  }
}
